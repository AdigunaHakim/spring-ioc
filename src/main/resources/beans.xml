<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
	http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd">

    <bean id="humanBrain" class="com.example.Brain">
        <property name="noOfBrain" value="1"/>
    </bean>

    <bean id="catBrain" class="com.example.Brain">
        <property name="noOfBrain" value="1"/>
    </bean>

    <bean id="heart" class="com.example.Heart">
        <property name="noOfHeart" value="1"/>
    </bean>

    <bean id="lungs" class="com.example.Lungs">
        <property name="noOfLungs" value="2"/>
    </bean>

    <bean id="humanBody" class="com.example.Body" autowire="byType"/>
</beans>